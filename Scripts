
SELECT
    SUM(fs.UnitsSold) AS TotalUnitsSold
FROM FactSales fs
JOIN DimCalendar cal ON cal.CalendarKey = fs.CalendarKey
JOIN DimProduct p    ON p.ProductKey = fs.ProductKey
JOIN DimStore s      ON s.StoreKey = fs.StoreKey
WHERE
    cal.DayOfWeek = 'Wednesday'
    AND cal.Year = 2024
    AND cal.Quarter = 1
    AND p.ProductCategoryName = 'Footwear'
    AND p.ProductVendorName = 'Mountain King'
    AND s.StoreRegionName = 'Chicagoland';
